            {%- when 'product_details' -%}

              <div class="product__info product__block-{{- block.type }} mb-32" {{ block.shopify_attributes }}>
                <sht-accordion data-properties='{"multiple": false}'>
                  <div class="accordion">
                    <div class="accordion__item js-accordion-item" {{ block.shopify_attributes }}>
                      <div class="accordion__header wb-break-word">
                        <button
                          class="accordion__button product__desc__accordion-button w-100 p-zero js-accordion-trigger d-flex middle-xs p-relative pt-8 pb-8 c-pointer gap-12 {{ block.settings.expand_default | replace: true, 'open' }}"
                          id="acdnTgr-{{- block.id -}}"
                          aria-expanded="{{- block.settings.expand_default -}}"
                          aria-controls="acdnCtt-{{- block.id -}}"
                        >

                          <span class="m-zero flex-1 ta-left fw-400">{{- block.settings.heading -}}</span>
                          <span class="accordion__toggle zi-1 d-flex btn  btn--medium bg-transparent">
                          
                          <svg class="accordion-button__icon" width="14" height="8" viewBox="0 0 14 8" fill="none" xmlns="http://www.w3.org/2000/svg">
                          <path d="M1.03711 1L7.03711 7L13.0371 1" stroke="#ADADAD" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                          </svg>

                          
                          </span>
                        </button>
                      </div>
                      <div
                        class="accordion__content o-hidden rte js-accordion-content accordion-content ac-{{- block.id }} {{ block.settings.expand_default | replace: true, 'open' }}"
                        id="acdnCtt-{{- block.id }}"
                        role="region"
                        aria-describedby="acdnTgr-{{- block.id -}}"
                        tabindex="-1"
                        style="{{ block.settings.expand_default | default: 'max-height: 0;' | replace: true, '' }}"
                      >
                        {{- product.description -}}
                      </div>
                    </div>
                  </div>
                </sht-accordion>
              </div>


            {%- when 'collapsible_item' -%}
              <div class="product__info product__block-{{- block.type }} mb-32" {{ block.shopify_attributes }}>
                <sht-accordion data-properties='{"multiple": false}'>
                  <div class="accordion">
                    <div class="accordion__item js-accordion-item" {{ block.shopify_attributes }}>
                      <div class="accordion__header wb-break-word">
                        <button
                          class="accordion__button product__desc__accordion-button w-100 p-zero js-accordion-trigger d-flex middle-xs p-relative pt-8 pb-8 c-pointer gap-12 {{ block.settings.expand_default | replace: true, 'open' }}"
                          id="acdnTgr-{{- block.id -}}"
                          aria-expanded="{{- block.settings.expand_default -}}"
                          aria-controls="acdnCtt-{{- block.id -}}"
                        >

                          <span class="m-zero flex-1 ta-left fw-400">{{- block.settings.heading -}}</span>
                          <span class="accordion__toggle zi-1 d-flex btn btn-icon btn--medium bg-transparent">
                            {%- render 'icon-chevron-down', class: 'accordion-button__icon' -%}
                          </span>
                        </button>
                      </div>
                      <div
                        class="accordion__content o-hidden rte js-accordion-content accordion-content ac-{{- block.id }} {{ block.settings.expand_default | replace: true, 'open' }}"
                        id="acdnCtt-{{- block.id }}"
                        role="region"
                        aria-describedby="acdnTgr-{{- block.id -}}"
                        tabindex="-1"
                        style="{{ block.settings.expand_default | default: 'max-height: 0;' | replace: true, '' }}"
                      >
                        {{- block.settings.description -}}
                      </div>
                    </div>
                  </div>
                </sht-accordion>
              </div>
            {%- when 'pickup_availability' -%}
              {%- liquid
                if block.settings.enable_item_background
                  unless block.settings.color_content.alpha == 0.0 or block.settings.color_content == blank
                    assign color_content = 'color: ' | append: block.settings.color_content | append: '; --color-normal-text-rgb:' | append: block.settings.color_content.red | append: ',' | append: block.settings.color_content.green | append: ',' | append: block.settings.color_content.blue
                  endunless
                  unless block.settings.background_content.alpha == 0.0 or block.settings.background_content == blank
                    assign background_content = '; background: ' | append: block.settings.background_content
                  endunless
                endif
              -%}
              {%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities
                | where: 'pick_up_enabled', true
              -%}
              {%- if pick_up_availabilities.size > 0 -%}
                <div
                  class="product__info mb-32 product__block-{{- block.type }} hide-empty br-card-content o-hidden {{ block.settings.show_pickup_time | default: '' | replace: 'true', 'pickup_availability--show-pickup-time' }} {{ block.settings.enable_item_background | default: 'card--standard' | replace: 'true', 'border' }} "
                  {{ block.shopify_attributes }}
                >
                  <sht-pickup-avail
                    class="pickup-availability d-block no-js-hidden p-relative card__content"
                    style="{{ color_content }} {{ background_content }}"
                    {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %}
                      available
                    {% endif %}
                    data-base-url="{{ shop.url }}{{ routes.root_url }}"
                    data-variant-id="{{ product.selected_or_first_available_variant.id }}"
                    data-has-only-default-variant="{{ product.has_only_default_variant }}"
                    data-show-expected-pickup-time="{{ block.settings.show_pickup_time }}"
                  >
                    <template>
                      <sht-pickup-avail-prwv class="d-flex top-xs pickup-availability__preview">
                        {% render 'icon-check' %}
                        <div class="pickup-availability__info">
                          <p>{{- 'products.product.pickup_availability.unavailable' | t -}}</p>
                          <button class="btn btn-small btn-plain">
                            {{- 'products.product.pickup_availability.refresh' | t -}}
                          </button>
                        </div>
                      </sht-pickup-avail-prwv>
                    </template>
                  </sht-pickup-avail>
                </div>
              {%- endif -%}
            {%- when 'popup_content' -%}
              {%- liquid
                assign popup_content_dialog_id = block.id | append: section.id | md5 | truncate: 12, ''
                assign popup_content_block = block
                assign size_popup_content_cta = 'disabled'
                if block.settings.static_page != blank
                  assign size_popup_content_cta = ''
                endif
              %}
              <div class="product__info product__block-{{- block.type }} mb-32 mt-24" {{ block.shopify_attributes }}>
                <div class="product__popup-header-wrap"> 
                 {% if block.settings.custom_icon != blank %}
                  <span class="acc_custom-icon">{{ block.settings.custom_icon }}</span>
                {% endif %}
                <button
                  class="btn-popup_content fs-body bs-none  pt-8 pb-8 c-pointer"
                  type="button"
                  aria-haspopup="dialog"
                  onclick="window.dialogPopupContent{{ popup_content_dialog_id }}.showModal(this, true)"
                  {{ size_popup_content_cta }}
                >
                  {{- block.settings.title -}}
                </button>
                </div>
              </div>
              {% render 'product-popup-content',
                popup_content_dialog_id: popup_content_dialog_id,
                popup_content_block: popup_content_block
              %}
            {%- when 'custom_text' -%}
              <div
                class="product__info product__block-{{- block.type }} rte mb-32 hide-empty"
                {{ block.shopify_attributes }}
                {%- unless block.settings.color_content.alpha == 0.0 or block.settings.color_content == blank -%}
                  style="color: {{ block.settings.color_content }};"
                {%- endunless -%}
              >
                {{ block.settings.description }}
              </div>
            {%- when 'custom_liquid' -%}
              <div
                class="product__info product__block-{{- block.type }} hide-empty mb-32"
                {{ block.shopify_attributes }}
              >
                {{- block.settings.custom_liquid -}}
              </div>
            {%- when 'complementary_product' -%}
              <sht-prd-recom
                class="product__info complementary-products d-block mb-32"
                data-url="{{ routes.product_recommendations_url }}?section_id={{ section.id }}&product_id={{ product.id }}&intent=complementary"
                {{ block.shopify_attributes }}
              >
                {%- if recommendations.performed and recommendations.products_count > 0 -%}
                  <span class="d-block mb-24 fw-700">{{ block.settings.heading }}</span>
                  <div class="cross-sell__items">
                    <sht-carousel class="sht-carousel p-relative d-block w-100" data-enable="true">
                      <div
                        aria-atomic="true"
                        class="carousel-container rw r-gap-equal carousel js-carousel-items ox-auto p-relative scrollable scrollable-x disable-scrollbars"
                      >
                        {%- for recommendation in recommendations.products limit: block.settings.product_to_show -%}
                          <sht-carousel-itm
                            class="carousel-item clm js-carousel-item p-relative scrollable-aln-start"
                            style="--w-xs:12; --w-sm:6;"
                            role="group"
                            aria-roledescription="Slide"
                            aria-label="{{ 'accessibility.slide' | t }} {{ forloop.index }} {{ 'accessibility.of' | t }} {{ forloop.length }}"
                            data-item-index="{{ forloop.index }}"
                          >
                            {%- render 'card-product',
                              product: recommendation,
                              show_vendor: true,
                              show_color_swatch: false,
                              ratio: 1,
                              add_to_cart: block.settings.quick_shop,
                              card_content_position: 'ta-left start-xs',
                              image_width: '320, 450',
                              section: section,
                              card_layout: 'horizontal border'
                            -%}
                          </sht-carousel-itm>
                        {%- endfor -%}
                      </div>
                      <div class="carousel-pagination middle-y w-100 p-absolute no-js-hidden d-none">
                        <div class="p-absolute left-0 middle-y">
                          <button
                            type="button"
                            data-step="1"
                            class="js-carousel-prev-btn btn btn-outline  btn-icon icon btn--medium"
                            aria-label="{{ 'accessibility.previous_slide' | t }}"
                            name="previous"
                            disabled
                          >
                            {% render 'icon-chevron-left' %}
                          </button>
                        </div>
                        <div class="p-absolute right-0 middle-y">
                          <button
                            type="button"
                            data-step="1"
                            class="js-carousel-next-btn btn btn-outline  btn-icon icon btn--medium"
                            aria-label="{{ 'accessibility.next_slide' | t }}"
                            name="next"
                            disabled
                          >
                            {% render 'icon-chevron-right' %}
                          </button>
                        </div>
                      </div>
                    </sht-carousel>
                  </div>
                {%- endif -%}
              </sht-prd-recom>
            {%- when 'share' -%}
              <div
                class="product__info product__block-{{- block.type }} product__block-{{- block.type }} d-flex mb-32"
                {{ block.shopify_attributes }}
              >
                <sht-share-prd-btn
                  class="d-flex middle-xs gap-12"
                  data-product-url="{{ product.selected_variant.url | default: product.url | prepend: request.origin }}"
                >
                  <span class="fw-700">{{- 'accessibility.share' | t -}}</span>
                  <div class="js-web-share-api-container" hidden>
                    <button
                      class="btn btn-icon btn--medium {{ block.settings.button_style }} js-web-share-api-btn"
                      aria-label="{{ 'accessibility.social_share' | t }}"
                    >
                      {% render 'icon-share' %}
                    </button>
                  </div>
                  <div
                    class="js-none-web-share-api-container"
                    hidden
                    id="productShareDetails-{{ block.id }}-{{ section.id }}"
                  >
                    {%- render 'product-social-share', product: product, icon_style: block.settings.button_style -%}
                  </div>
                </sht-share-prd-btn>
              </div>
          {% endcase %}
        {% endfor %}
      </div>
    </div>
  </div>
</div>

{%- liquid
  if section.settings.enable_sticky_add_to_cart
    render 'product-sticky-purchasing-bar', product: product, sticky_product_form_id: sticky_product_form_id, section: section, product_form_id: product_form_id, purchasing_bar_position: section.settings.add_to_cart_sticky_position
  endif
  if size_guide_block
    render 'product-size-guide', size_guide_dialog_id: size_guide_dialog_id, size_guide_block: size_guide_block, size_guide_heading: size_guide_block.settings.size_guide_label
  endif
  render 'main-product-gallery-dialog', product: product, rand: section_id, ratio: 1, object_fit: object_fit, media_background: section.settings.media_background_color
-%}
{%- liquid
  render 'js-loader', files: "component-add-to-cart.js" | split: ', '
  render 'product-google-snippet', product: product
-%}
