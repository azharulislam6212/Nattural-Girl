                      assign loading = 'lazy'
                      assign decoding = 'async'
                      assign fetchpriority = 'auto'

                      assign custom_img = block.settings[custom_img_c]
                      assign heading = block.settings[heading_c]

                      assign custom_icon = custom_img | image_url: width: block.settings.custom_img.width | image_tag: widths: '120', sizes: '(min-width: 361px) calc(100vw/2), calc(100vw/3)', alt: custom_img.alt, class: 'media of-cover h-100 w-100', loading: loading, fetchpriority: fetchpriority, decoding: decoding
                    %}

