                                      data-type: 'add-to-cart-form'
                                    -%}
                                      <div class="form__field">
                                        <input
                                          type="hidden"
                                          class="form__control"
                                          name="id"
                                          value="{{ product.selected_or_first_available_variant.id }}"
                                          disabled
                                        >
                                      </div>
                                      <div class="product-form__buttons d-flex top-xs fd-column">
                                        <div class="gap-12 d-flex w-100 fd-column">
                                          <button
                                            type="submit"
                                            name="add"
                                            class="js-product-form-submit-btn product-btn-atc btn btn-primary btn-custom  btn-custom--large w-100 fw-700"
                                            {% if product.selected_or_first_available_variant.available == false %}
                                              disabled
                                              {% assign add_to_cart_btn_lbl = 'products.product.sold_out' | t %}
                                            {% else %}
                                              {% assign add_to_cart_btn_lbl = 'products.product.add_to_cart' | t %}
                                            {% endif %}
                                          >
                                            <span class="p-relative d-flex middle-xs center-xs ta-center">
                                              <span class="js-product-form-submit-btn-text wb-break-word">
                                                {{ add_to_cart_btn_lbl }} - {{ product.price | money }}
                                              </span>
                                              <span class="js-product-form-spinner product-form-spinner p-absolute icon">
                                                {%- render 'icon-spinner', size: '24px' -%}
                                              </span>
                                            </span>
                                          </button>
                                        </div>
                                      </div>
                                    {%- endform -%}
                                  </sht-prd-frm>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      {% endfor %}
                    </div>

                    {% if enable_navigation %}
                      <div class="swiper-navigation-wrapper  color-{{ section.settings.secondary_color_scheme }}">
                        <div class="swiper-button-prev">
                          <svg
                            width="14"
                            height="14"
                            viewBox="0 0 14 14"
                            fill="none"
                            xmlns="http://www.w3.org/2000/svg"
                          >
                            <path d="M13 7H1" stroke="currentColor" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M7 13L1 7L7 1" stroke="currentColor" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
                          </svg>
                        </div>

                        <div class="swiper-button-next">
                          <svg
                            width="14"
                            height="14"
                            viewBox="0 0 14 14"
                            fill="none"
                            xmlns="http://www.w3.org/2000/svg"
                          >
                            <path d="M1 7L13 7" stroke="currentColor" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M7 1L13 7L7 13" stroke="currentColor" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
                          </svg>
                        </div>
                      </div>
                    {% endif %}

                    {% if enable_pagination %}
                      <div class="pagination-wrap">
                        <div class="swiper-pagination color-{{ section.settings.secondary_color_scheme }}"></div>
                      </div>
                    {% endif %}

                    {% if enable_scroll %}
                      <div class="swiper-scrollbar"></div>
                    {% endif %}
                  </div>
                </slideshow-swiper>

              {% endif %}
              </div>
            {% endif %}
            
