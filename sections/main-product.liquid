                          aria-expanded="{{- block.settings.expand_default -}}"
                          aria-controls="acdnCtt-{{- block.id -}}"
                        >

                          <span class="m-zero flex-1 ta-left fw-400">{{- block.settings.heading -}}</span>
                          <span class="accordion__toggle zi-1 d-flex btn btn--medium bg-transparent">
                            <svg class="accordion-button__icon" width="14" height="8" viewBox="0 0 14 8" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M1.03711 1L7.03711 7L13.0371 1" stroke="#ADADAD" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
