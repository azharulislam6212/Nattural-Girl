                    data-centeredslides="false"
                    data-slidesDesktop="1"
                    data-slidesTab="1"
                    data-slidesMobile="1"
                    data-autoPlay="{{ autoPlay }}"
                    data-delay="{{ delay }}"
                    data-loop="{{ loop }}"
                    data-freemode="false"
                    data-freemomentum="false"
                    data-mousewheel="false"
                    data-desktopspace="16"
                    data-tabspace="16"
                    data-mobilespace="16"
                    data-type="{{pagination_type}}"
                  >
                    <div
                      class=" swiper-wrapper"
                    >
                      {% for product in products %}
                        {% liquid
                          assign product_form_id = 'productForm-' | append: block.id
                        %}
