                        data-view-less="{{ 'sections.main_product.read_more.view_less' | t }}"
                      >
                        {{ 'sections.main_product.read_more.view_more' | t }}
                      </button>
                    </div>
                  </sht-read-more>
                {% endif %}
              </div>
            {%- when 'custom_icon_text' -%}
              <div class="custom-icon-text color-{{ block.settings.color_scheme }} gradient text-center w-100">
