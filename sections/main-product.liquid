              <div class="upsell-section">

                {% if upsells_sections.upsells_heading != blank %}
                  <h4 class="upsell__heading">{{ upsells_sections.upsells_heading }}</h4>
                {% endif %}

                {% if products != blank %}
                  
             

                <slideshow-swiper>
                  <div
