                                  aria-label="{{ 'accessibility.plus' | t }}"
                                  name="plus"
                                  data-name="plus"
                                  type="button"
                                >
                                  {%- render 'icon-plus', size: 20 -%}
                                </button>
                              </sht-qty-inp>
                            </div>
                            <button
                              type="submit"
                              name="add"
                              class="js-product-form-submit-btn product-btn-atc btn btn-primary btn-custom  btn-custom--large w-100 fw-700"
                              {% if product.selected_or_first_available_variant.available == false %}
                                disabled
                                {% assign add_to_cart_btn_lbl = 'products.product.sold_out' | t %}
                              {% else %}
                                {% assign add_to_cart_btn_lbl = 'products.product.add_to_cart' | t %}
                              {% endif %}
                            >
                              <span class="spakle-btn-left">
                                <img src="{{ 'twinkles.gif' | asset_url }}" alt="twinkles">
                              </span>

                              <span class="p-relative d-flex middle-xs center-xs ta-center">
                                <span class="js-product-form-submit-btn-text wb-break-word">
                                  {{ add_to_cart_btn_lbl }} - {{ product.price | money }}
                                </span>
                                <span class="js-product-form-spinner product-form-spinner p-absolute icon">
                                  {%- render 'icon-spinner', size: '24px' -%}
                                </span>
                              </span>

                              <span class="spakle-btn-right">
                                <img src="{{ 'twinkles.gif' | asset_url }}" alt="twinkles">
                              </span>
                            </button>
                          </div>
                          {% if block.settings.show_dynamic_checkout_buttons %}
                            <sht-load-dyn-co-btn class="mt-12"
                              ><template>{{ form | payment_button }}</template></sht-load-dyn-co-btn
                            >
                          {% endif %}
                        </div>
                      {%- endform -%}
                    </sht-prd-frm>
                  </div>
                </div>
                {%- if block.settings.show_payment_method and shop.enabled_payment_types -%}
                  <div class="product__payment-method mt-24 wb-break-word">
                    <span class="d-block fw-700 mb-16">{{- block.settings.payment_method_title -}}</span>
                    <ul class="list list-payment d-flex gap-8 fw-wrap ls-none">
                      {%- for type in shop.enabled_payment_types -%}
                        <li class="list-payment__item d-flex">
                          {{ type | payment_type_svg_tag }}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </div>
                {%- endif -%}
              </sht-sticky-prd-info>
            {%- when 'description' -%}
              <div
                class="product__info product__block-{{- block.type }} {% if product.description == blank %}d-none{% endif %}"
                {{ block.shopify_attributes }}
              >
                {% if block.settings.description_type == 'collapsible' %}
                  <sht-accordion data-properties='{"multiple": false}'>
                    <div class="accordion">
                      <div class="accordion__item js-accordion-item" {{ block.shopify_attributes }}>
                        <div class="accordion__header wb-break-word">
                          <button
                            class="accordion__button product__desc__accordion-button w-100 p-zero js-accordion-trigger d-flex middle-xs p-relative pt-8 pb-8 c-pointer gap-12"
                            id="acdnTgr-{{- block.id -}}"
                            aria-expanded="false"
                            aria-controls="acdnCtt-{{- block.id -}}"
                          >
                            <span class="m-zero flex-1 ta-left fw-400">{{ 'products.product.description' | t }}</span>
                            <span class="accordion__toggle zi-1 d-flex btn btn-icon btn--medium bg-transparent">
