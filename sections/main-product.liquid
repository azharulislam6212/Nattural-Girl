{%- liquid
  assign section_id = section.id | md5 | truncate: 12, ''
  assign product_form_installment_id = 'productFormInstallment-' | append: section.id
  assign variant_label = settings.variant_swatches_option | downcase | default: 'color'

  assign thumb_index = 0
  assign thumbnails_ratio = section.settings.thumbnails_ratio | replace: 'fit', '' | replace: 'fill', ''
  assign object_fit = 'of-cover'
  if section.settings.thumbnails_ratio contains 'fit'
    assign object_fit = 'of-contain'
  endif
  assign carousel_desktop = true
  if section.settings.media_layout == 'carousel'
    assign carousel_desktop = false
  endif
  assign hide_variant_image = true
  if section.settings.media_layout == 'grid'
    assign hide_variant_image = false
  endif
  assign is_first_image = false

  assign media_background = section.settings.media_background_color

  assign product_form_id = 'productForm-' | append: section.id
  assign sticky_product_form_id = 'productStickyForm-' | append: section.id
  assign product_form_installment_id = 'productFormInstallment-' | append: section.id
  assign size_guide_block = null
  assign size_guide_dialog_id = ''
  assign first_3d_model = product.media | where: 'media_type', 'model' | first

  assign clm_img = 6
  assign clm_content = 6
  if section.settings.media_size == 'large'
    assign clm_content = 12 | times: 0.4
    assign clm_img = 12 | times: 0.7
  endif
-%}
{%- if first_3d_model -%}
  <script type="application/json" id="ProductJSON-{{ product.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{%- endif -%}

{% liquid
  assign detect_background = 1
  if section.settings.color_scheme.settings.color_background.alpha != 0.0
    assign no_transparent = 'no-transparent'
    assign detect_background = section.settings.color_scheme.settings.color_background | color_brightness | times: 9999
  endif
%}
<style>
  #shopify-section-{{ section.id }} {
    --section-background-hash: {{ detect_background }};
  }
  #shopify-section-{{ section.id }} + * {
    --previous-section-background-hash: {{ detect_background }};
  }
</style>

<div
  class="s--margin color-{{- section.settings.color_scheme }} s-container-radius main-product-section {{ no_transparent }}"
  data-ui-component="Main Product Section"
  id="{{ section.id }}"
>
  <div class="main-product ctnr p-relative section ">

  {% assign choose_category = product.metafields.custom.choose_category.value %}


    {% if choose_category %}
    
    <div class="category-breadcrumb">
      <ul class="breadcrumb-list">
        <li> <a href="/collections/all">Collections </a></li>
        <li>
          <svg width="7" height="12" viewBox="0 0 7 12" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M1 11L6 6L1 1" stroke="#999999" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>
        </li>
        <li>
          <a href="{{ choose_category.url }}" class="active">{{ choose_category.title }}</a>
        </li>
      </ul>
    </div>
  {% endif %}


    <div class="section-gap-md main-product--media-{{- section.settings.media_size }} d-flex fd-row-md fd-column top-xs">
      <div
        class="main-product__media maw-100 w-100 {{ section.settings.enable_media_sticky | replace: 'true', 'p-sticky-md top-0' }}"
        role="region"
        aria-label="{{- 'accessibility.view_gallery' | t -}}"
        style="--w-xs:12; --w-md:{{- clm_img -}}"
      >
        <a
          class="visually-hidden skip-to-content btn btn-primary  d-block ta-center"
          href="#productInfo-{{ section.id }}"
        >
          {{- 'accessibility.skip_to_product_information' | t -}}
        </a>
        <div class="product__media-wrapper--single-with-thumbnails">
          <sht-prd-slideshow
            role="status"
            class="product-slideshow product__media--{{ section.settings.media_layout }} d-grid p-relative gap-2x"
            data-hide-variant-image="{{- hide_variant_image -}}"
            data-layout="{{ section.settings.media_layout }}"
            data-section="{{ section.id }}"
          >
            <div class="product-slideshow__thumbnail product-slideshow__thumbnail--vertical d-flex-md middle-xs no-js-hidden maw-100 o-scroll disable-scrollbars {{ section.settings.enable_thumbnail_mobile | replace: 'false', 'd-none' }}">
              <sht-vert-carousel
                class="sht-carousel product-thumbs-{{ section.settings.media_layout | replace: 'thumbnail_left', 'carousel--vertical' | replace: 'thumbnail_bottom', 'carousel--horizontal' }}  p-relative"
                data-section="{{ section.id }}"
                data-layout="{{ section.settings.media_layout | replace: 'thumbnail_left', 'carousel-vertical' | replace: 'thumbnail_bottom', 'carousel-horizontal' }}"
              >
                <div
                  class="product-slideshow__thumbs product__{{ section.settings.thumbnails_ratio | replace: '2/3', 'thumbnails--portrait' | replace: '3/2', 'thumbnails--landscape' | replace: '1', 'thumbnails--square' }} h-100 top-xs js-carousel-items vertical-carousel vertical-carousel__container d-flex middle-xs p-relative disable-scrollbars fw-nowrap scrollable scrl-bhvr-auto carousel_{{ section.settings.media_layout }}"
                  role="list"
                  aria-label="{{ 'accessibility.slider' | t }}"
                >
                  {%- liquid
                    if product.selected_or_first_available_variant.featured_media != null
                      assign thumb_index = thumb_index | plus: 1
                      assign featured_media = product.selected_or_first_available_variant.featured_media
                      render 'product-slideshow-thumb', section: section, media: featured_media, product: product, thumb_index: thumb_index, ratio: thumbnails_ratio, object_fit: object_fit, media_background: media_background
                    endif
                    for media in product.media
                      assign thumb_index = thumb_index | plus: 1
                      unless media.id == product.selected_or_first_available_variant.featured_media.id
                        render 'product-slideshow-thumb', section: section, media: media, product: product, thumb_index: thumb_index, ratio: thumbnails_ratio, object_fit: object_fit, media_background: media_background
                      endunless
                    endfor
                  -%}
                </div>
                <div class="product-slideshow__pagination {{ section.settings.media_layout | replace: 'thumbnail_left', 'carousel-pagination--vertical' | replace: 'thumbnail_bottom', 'carousel-pagination--horizontal' }} carousel-controls--outside carousel-pagination d-flex-md d-none-important center-xs no-js-hidden js-carousel-pagination p-absolute top-0 h-100">
                  <div class="product-slideshow__pagination-btn d-none d-block-md btn-prev p-absolute top-0">
                    <button
                      type="button"
                      data-step="1"
                      class="btn btn-outline btn--medium btn-icon js-carousel-prev-btn"
                      aria-label="{{ 'accessibility.previous_slide' | t }}"
                      name="previous"
                      disabled="true"
                    >
                      {% render 'icon-chevron-left' %}
                    </button>
                  </div>
                  <div class="product-slideshow__pagination-btn d-none d-block-md btn-next p-absolute bottom-0">
                    <button
                      type="button"
                      data-step="1"
                      class="btn btn-outline btn--medium btn-icon js-carousel-next-btn"
                      aria-label="{{ 'accessibility.next_slide' | t }}"
                      name="next"
                    >
                      {% render 'icon-chevron-right' %}
                    </button>
                  </div>
                </div>
              </sht-vert-carousel>
            </div>
            <div class="product-slideshow__slide h-100 d-flex middle-xs flex-1 p-relative product-slideshow-mobile-full-width">
              {% render 'product-gallery-carousel',
                product: product,
                section: section,
                rand: section_id,
                hide_variant_image: hide_variant_image,
                is_first_image: is_first_image,
                open_zoom_modal: section.settings.enable_media_zoom,
                ratio: section.settings.main_media_ratio,
                force_eager: true,
                media_background: media_background,
                carousel_desktop: carousel_desktop
              %}
            </div>
          </sht-prd-slideshow>
        </div>
        {% liquid
          assign tab = section.blocks | where: 'type', 'tab'
          assign product_details = section.blocks | where: 'type', 'product_details'
        %}

        {% if tab!= blank  or product_details != blank %}

          {% assign product_details_tab = true %}
          {% for block in section.blocks %}
            {%- case block.type -%}
            {%- when 'product_details' -%}
           {% assign product_details_tab = false %}
          {% endcase %}
        {% endfor %}


        <div class="tab-area">
          <ul class="product__tabs">
            {% assign count = 1 %}
            {% for block in section.blocks %}
              {%- case block.type -%}
                {%- when 'tab' -%}
                  <li class="tab_link {%if count == 1 and product_details_tab %} active {% endif %}" data-tab="tab-{{ block.id }}">
                    {{ block.settings.heading }}
                    <span class="sparkle-icon">
                      <svg
                        class="sparkle"
                        width="28"
                        height="22"
                        viewBox="0 0 28 22"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                      >
                        <path d="M8.0635 12C6.10885 16.3696 6.27439 17.0567 10 20.0635C5.6304 18.1088 4.94332 18.2744 1.93652 22C3.89117 17.6304 3.72563 16.9433 0 13.9365C4.36962 15.8912 5.05669 15.7256 8.0635 12Z" fill="#8E2210"/>
                        <path d="M24.7079 0C21.385 7.42835 21.6665 8.59638 28 13.7079C20.5717 10.385 19.4036 10.6664 14.2921 17C17.615 9.57165 17.3336 8.40362 11 3.29206C18.4283 6.61496 19.5964 6.33356 24.7079 0Z" fill="#8E2210"/>
                      </svg>
                    </span>
                  </li>
                  {% assign count = count | plus: 1 %}
                {%- when 'product_details' -%}
                  <li class="tab_link active " data-tab="tab-{{ block.id }}">
                    {{ block.settings.heading }}
                    <span class="sparkle-icon">
                      <svg
                        class="sparkle"
                        width="28"
                        height="22"
                        viewBox="0 0 28 22"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                      >
                        <path d="M8.0635 12C6.10885 16.3696 6.27439 17.0567 10 20.0635C5.6304 18.1088 4.94332 18.2744 1.93652 22C3.89117 17.6304 3.72563 16.9433 0 13.9365C4.36962 15.8912 5.05669 15.7256 8.0635 12Z" fill="#8E2210"/>
                        <path d="M24.7079 0C21.385 7.42835 21.6665 8.59638 28 13.7079C20.5717 10.385 19.4036 10.6664 14.2921 17C17.615 9.57165 17.3336 8.40362 11 3.29206C18.4283 6.61496 19.5964 6.33356 24.7079 0Z" fill="#8E2210"/>
                      </svg>
                    </span>
                  </li>

                  {% assign count = count | plus: 1 %}
              {% endcase %}
       
            {% endfor %}
          </ul>

          {% comment %} <div class="progress">
          <div class="bar" style="width:35%">
            <p class="percent">35%</p>
          </div>
        </div> {% endcomment %}

          {% assign count = 1 %}
     

          {% for block in section.blocks %}
            {%- case block.type -%}
              {%- when 'tab' -%}
                <div id="tab-{{ block.id }}" class="tab-content slide-read-more {%if count == 1 and product_details_tab %} current {% endif %}">

                {% if block.settings.page_content != blank %}
                  {% assign content =   pages[block.settings.page_content].content %}
                  {% else %}
                  {% assign content = block.settings.description %}
                  
                {% endif %}
                  
                  <div class="content-wrap"> 
                  {{ content }}
                  </div>

                {% assign length = content | size  %}

                  {% if length > 550 %}
                    
                
                  <button class="readmore" style="text-decoration: underline;">See more </button>

                {% endif %}
                </div>

                {% assign count = count | plus: 1 %}
              {%- when 'product_details' -%}
                <div id="tab-{{ block.id }}" class="tab-content   slide-read-more  current ">
                  <div class="content-wrap"> 
                {{ product.description}}
                </div>
                {% assign length = product.description | size  %}

                {% if length > 550 %}

                  <button class="readmore" style="text-decoration: underline;">See more </button>

                {% endif %}
                </div>

                {% assign count = count | plus: 1 %}
            {% endcase %}
          {% endfor %}
        </div>
      {% endif %}

      </div>
      <div
        id="productInfo-{{ section.id }}"
        class="main-product__info d-flex fw-wrap maw-100"
        style="--w-xs:12; --w-md:{{- clm_content -}}"
      >
        {% for block in section.blocks %}
          {%- case block.type -%}
            {%- when '@app' -%}
              <div class="product__info product__info--app product__block-{{- block.type }}">
                {% liquid
                  render block
                %}
              </div>
            {%- when 'title' -%}
              <div class="product__info product__block-{{- block.type }} mb-24" {{ block.shopify_attributes }}>
                <h1 class="product__title {{ block.settings.heading_size }}  m-zero">{{- product.title | escape -}}</h1>
              </div>
            {% when 'product_code' %}
              <div
                class="product__info product__block-{{- block.type }} d-flex fd-column mb-16 hide-empty"
                id="sku-{{- section.id }}"
                role="status"
                {{ block.shopify_attributes }}
              >
                {%- if product.selected_or_first_available_variant.sku != blank and block.settings.show_sku -%}
                  <span class="variant-sku subtext mb-16"
                    ><span class="variant-sku__label fw-b">SKU: </span>
                    {{- product.selected_or_first_available_variant.sku -}}
                  </span>
                {%- endif -%}
                {%- if product.selected_or_first_available_variant.barcode != blank and block.settings.show_barcode -%}
                  <span class="variant-bar-code subtext"
                    ><span class="variant-sku__label fw-b">Barcode: </span>
                    {{- product.selected_or_first_available_variant.barcode -}}
                  </span>
                {%- endif -%}
              </div>
            {%- when 'vendor' -%}
              <div class="product__info product__block-{{- block.type }} mb-24" {{ block.shopify_attributes }}>
                <span class="visually-hidden">{{- 'accessibility.vendor' | t -}}</span>
                <span class="d-block subtext color-light wb-break-word">{{- product.vendor -}}</span>
              </div>
            {%- when 'price' -%}
              {% liquid
                if block.settings.enable_background
                  unless block.settings.color_text.alpha == 0.0 or block.settings.color_text == blank
                    assign color_text = 'color: ' | append: block.settings.color_text | append: ';--color-normal-text-rgb:' | append: block.settings.color_text.red | append: ',' | append: block.settings.color_text.green | append: ',' | append: block.settings.color_text.blue
                  endunless
                  unless block.settings.color_background.alpha == 0.0 or block.settings.color_background == blank
                    assign color_background = '; background: ' | append: block.settings.color_background
                  endunless
                endif
              %}
              <div
                class="product__info product__block-{{- block.type }} mb-24   block-rounded custom-text {{ block.settings.enable_background | default: 'card--standard' | replace: 'true', '' }}"
                {{ block.shopify_attributes }}
                style="{{ color_text }}; {{ color_background }}"
              >
                <div class="card__content-price">
                  <div class="d-flex middle-xs fw-wrap gap-8" id="productPrice-{{- section.id }}" role="status">
                    {%- render 'product-price',
                      product: product,
                      target: product.selected_or_first_available_variant,
                      price_class: 'price--large',
                      price_sale_bold: '',
                      price_style: 'fw-700'
                    -%}

                   
                    <div class="product__badge d-flex fw-wrap gap-8 hide-empty">
                      {%- if product.selected_or_first_available_variant.compare_at_price
                          > product.selected_or_first_available_variant.price
                      -%}
                        {%- liquid
                          assign discount_cost = product.selected_or_first_available_variant.compare_at_price | minus: product.selected_or_first_available_variant.price
                          assign discount_percent = discount_cost | times: 100.0 | divided_by: product.selected_or_first_available_variant.compare_at_price | round: 0
                        -%}
                        <span class="badge sale-badge subtext d-inline-block p-relative fw-700">
                          {{- 'products.product.on_sale' | t }}
                          {{ discount_percent }}%
                          <span hidden>
                            {%- if settings.currency_code_enabled -%}
                              {{- discount_cost | money_with_currency -}}
                            {% else %}
                              {{- discount_cost | money -}}
                            {%- endif -%}
                          </span>
                        </span>
                      {%- endif -%}

                      {%- if product.selected_or_first_available_variant.available == false -%}
                        <span class="badge badge--soldout fw-700">
                          {{- 'products.product.sold_out' | t -}}
                        </span>
                      {%- endif -%}
                    </div>
                    <svg
                    class="sparkle price-sparcle"
                    width="30"
                    height="23"
                    viewBox="0 0 30 23"
                    fill="none"
                    xmlns="http://www.w3.org/2000/svg"
                  >
                    <path d="M25.9333 0C21.8286 9.1762 22.1762 10.6191 30 16.9333C20.8238 12.8286 19.381 13.1762 13.0667 21C17.1715 11.8238 16.8238 10.3809 9 4.06666C18.1762 8.17142 19.6191 7.82381 25.9333 0Z" fill="#8E2210"/>
                    <path d="M7.25715 14C5.49796 17.9327 5.64695 18.551 9.00001 21.2571C5.06736 19.498 4.44899 19.6469 1.74287 23C3.50205 19.0673 3.35306 18.449 0 15.7429C3.93266 17.502 4.55102 17.3531 7.25715 14Z" fill="#8E2210"/>
                  </svg>
                  </div>
                  {%- if block.settings.show_tax_and_shipping_policy -%}
                    <div class="mt-1x color-light hide-empty subtext rte">
                      {%- if cart.taxes_included -%}
                        {{- 'products.product.include_taxes' | t -}}
                        &nbsp;
                      {%- endif -%}
                      {%- if shop.shipping_policy.body != blank -%}
                        {{- 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url -}}
                      {%- endif -%}
                    </div>
                  {% endif %}
                  {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                    <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                    {{ form | payment_terms }}
                  {%- endform -%}
                </div>
              </div>
            {%- when 'variant' -%}
              {% liquid
                assign size_guide_dialog_id = block.id | append: section.id | md5 | truncate: 12, ''
                if block.settings.enable_size_guide
                  assign size_guide_block = block
                endif
                assign size_guide_cta = 'disabled'
                if block.settings.size_guide_content != blank
                  assign size_guide_cta = ''
                endif
              %}
              <div
                class="product__info product__block-{{- block.type }} mb-48 gap-24 d-flex fd-column"
                {{ block.shopify_attributes }}
                style="--variant-swatch-size: {{- block.settings.variant_swatches_size -}}"
              >
                {%- unless product.has_only_default_variant -%}
                  {%- if block.settings.variant_picker_type == 'button' -%}
                    <div id="variantPicker-{{ section.id }}">
                      <sht-variant-radios
                        class="product__variants--radios d-flex fd-column gap-24 no-js-hidden"
                        data-section="{{ section.id }}"
                        data-url="{{ product.url }}"
                      >
                      {% assign option_index = 0 %}
                        {%- for option in product.options_with_values -%}
                          {% assign index = forloop.index | minus: 1 %}
                          {%- assign variant_name = option.name | downcase -%}
                          <fieldset class="form__fields product-option js-variant-radio-container d-flex fw-wrap p-zero m-zero b-zero">
                            <legend class="p-zero m-zero product-option__name mb-8 d-flex center-xs between-xs w-100 middle-xs gap-8">
                              <span class="d-flex flex-1">
                                <span class="mr-8 ">{{ option.name }}
                                  
                                {% if option.name contains "Length" or option.name contains "length" or  option.name contains "Lengths" %}
                                  <span class="option-sub"> (Inches) </span>
                                {% endif %}
                
                                
                                </span>

                                <span
                                  class="js-variant-radio-option-name hide-empty fw-700"
                                  data-option-index="{{ index }}"
                                  data-option-name="{{ option.name }}"
                                >
                                  {{- product.selected_or_first_available_variant.options[index] -}}
                                </span>
                              </span>
                              {% if block.settings.enable_size_guide and variant_name == 'size' %}
                                <button
                                  class="btn btn-plain btn-link subtext fw-700 pt-8 pb-8 ta-right"
                                  type="button"
                                  aria-haspopup="dialog"
                                  onclick="window.dialogSizeGuide{{ size_guide_dialog_id }}.showModal(this, true)"
                                  {{ size_guide_cta }}
                                >
                                  <span class="subtext">
                                    {{ block.settings.size_guide_label }}
                                  </span>
                                </button>
                              {% endif %}
                            </legend>
                            {%- if variant_label == variant_name and block.settings.enable_variant_swatches -%}
                              {% render 'main-product-color-swatch',
                                product: product,
                                option: option,
                                section: section,
                                product_form_id: product_form_id,
                                index: index
                              %}
                            {%- else -%}
                              <ul class="ls-none d-flex fw-wrap gap-8">
                                {%- for value in option.values -%}
                                  <li
                                    class="form__field js-variant-option-value product-option__value {% if product.selected_or_first_available_variant.available == false and option.selected_value == value %}product-option--sold-out{% endif %}"
                                    data-value="{{ value | escape }}-{{ index }}"
                                    data-option-index="{{ index }}"
                                  >
                                    <input
                                      type="radio"
                                      class="form__control input--variant js-variant-radio-item js-variant-item visually-hidden"
                                      id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                                      name="{{ option.name }}"
                                      value="{{ value | escape }}"
                                      data-variant-value="{{ value | escape }}-{{ index }}"
                                      form="{{ product_form_id }}"
                                      {% if option.selected_value == value %}
                                        checked
                                      {% endif %}
                                    >
                                    <label
                                      class="form__label product-option__label d-block js-variant-option-label o-hidden p-relative c-pointer"
                                      for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                                    >
                                      <span class="product-variant-label">
                                        {{ value }}
                                      </span>
                                      <span class="product-option-v">
                                        <svg
                                          class="sparkle"
                                          width="10"
                                          height="10"
                                          viewBox="0 0 10 10"
                                          fill="none"
                                          xmlns="http://www.w3.org/2000/svg"
                                        >
                                          <path d="M8.0635 0C6.10885 4.36962 6.27439 5.05669 10 8.0635C5.6304 6.10885 4.94332 6.27437 1.93652 10C3.89117 5.63038 3.72563 4.94331 0 1.9365C4.36962 3.89115 5.05669 3.72563 8.0635 0Z" fill="#8E2210"/>
                                        </svg>
                                      </span>
                                    </label>
                                  </li>
                                {%- endfor -%}
                              </ul>
                            {%- endif -%}
                          </fieldset>
                        {%- endfor -%}
                        <script type="application/json">
                          {{ product.variants | json }}
                        </script>
                      </sht-variant-radios>
                    </div>
                  {%- else -%}
                    {% render 'main-product-variant-selects',
                      section: section,
                      product: product,
                      variant_label: variant_label,
                      block: block,
                      product_form_id: product_form_id,
                      enable_size_guide: block.settings.enable_size_guide,
                      size_guide_dialog_id: size_guide_dialog_id
                    %}
                  {%- endif -%}
                  <noscript>
                    <div class="form__fields {% if product.has_only_default_variant %} hidden{% endif %}">
                      <div class="form__field">
                        <label class="form__label" for="Variants-{{ section.id }}-{{ forloop.index0 }}">
                          {{- 'products.product.product_variants' | t -}}
                        </label>
                        <select
                          name="id"
                          id="Variants-{{ section.id }}-{{ forloop.index0 }}"
                          class="form__control w-100 h-100 field__text field--focus field--hover"
                          form="{{ product_form_id }}"
                        >
                          {%- for variant in product.variants -%}
                            <option
                              {% if variant == product.selected_or_first_available_variant %}
                                selected="selected"
                              {% endif %}
                              {% if variant.available == false %}
                                disabled
                              {% endif %}
                              value="{{ variant.id }}"
                            >
                              {{ variant.title }}
                              {%- if variant.available == false %}
                                - {{ 'products.product.sold_out' | t -}}
                              {%- endif -%}
                              - {{ variant.price | money | strip_html }}
                            </option>
                          {%- endfor -%}
                        </select>
                      </div>
                    </div>
                  </noscript>
                {%- endunless -%}
                <div
                  class="js-inventory-tracking hide-empty product__inventory-tracking hide-empty"
                  id="inventoryTracking-{{ section.id }}"
                  role="status"
                >
                  {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                    <span class="d-flex gap-8 subtext middle-xs">
                      {% if settings.product_information_low_stock_threshold > 0
                        and settings.product_information_low_stock_threshold
                          >= product.selected_or_first_available_variant.inventory_quantity
                      %}
                        <span class="btn-icon btn--small icon icon--warning">
                          {%- render 'icon-warning', size: '16px' -%}
                        </span>
                        {%- if block.settings.show_stock_number -%}
                          {{
                            'products.product.low_stock_html'
                            | t: stock: product.selected_or_first_available_variant.inventory_quantity
                          }}
                        {%- else -%}
                          {{ 'products.product.low_stock' | t }}
                        {%- endif -%}
                      {% else %}
                        <span class="btn-icon btn--small icon icon--success">
                          {%- render 'icon-check', size: '16px' -%}
                        </span>
                        {%- if block.settings.show_stock_number -%}
                          {{
                            'products.product.in_stock_html'
                            | t: stock: product.selected_or_first_available_variant.inventory_quantity
                          }}
                        {%- else -%}
                          {{ 'products.product.in_stock' | t }}
                        {%- endif -%}
                      {%- endif -%}
                    </span>
                  {%- else -%}
                    {%- if product.selected_or_first_available_variant.inventory_policy != 'continue'
                      and product.selected_or_first_available_variant.inventory_management != blank
                    -%}
                      <span class="d-flex gap-8 subtext middle-xs">
                        <span class="btn-icon btn--small icon icon--error">
                          {%- render 'icon-close', size: '16px' -%}
                        </span>
                        {{ 'products.product.out_of_stock' | t }}
                      </span>
                    {%- endif -%}
                  {%- endif -%}
                </div>
              </div>
            {%- when 'buy_buttons' -%}
              <sht-sticky-prd-info
